{
  "name": "vite-vue2",
  "version": "1.0.0",
  "scripts": {
    "serve": "vite preview",
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite",
    "lint:js": "eslint --ext \".js,.ts,.vue\" ./src",
    "lint:prettier": "prettier --check ./src",
    "lint": "npm run lint:js && npm run lint:prettier",
    "lint:fix": "prettier --write --list-different ./src && npm run lint:js --fix",
    "test": "vitest --dom --coverage",
    "icon-font": "fantasticon src/assets/icons -o public/icon-font -p mdi --normalize -t woff woff2 -g css"
  },
  "dependencies": {
    "unplugin-vue-components": "^0.19.6",
    "vite-plugin-vue2": "^1.9.3",
    "vue": "^2.6.14",
    "vuetify": "^2.6.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-env": "^7.18.6",
    "@vue/test-utils": "^1.3.0",
    "c8": "^7.11.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.1.1",
    "happy-dom": "^5.3.2",
    "sass": "~1.32",
    "typescript": "^4.5.4",
    "vite": "^2.9.12",
    "vitest": "^0.16.0",
    "vue-tsc": "^0.34.7"
  }
}
